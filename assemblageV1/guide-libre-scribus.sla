        <PAGEOBJECT XPOS="256.956010498688" YPOS="695.276535433071" OwnPage="1" ItemID="212580592" PTYPE="4" WIDTH="222.57217847769" HEIGHT="317.165354330709" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L222.572 0 L222.572 317.165 L0 317.165 L0 0 Z" copath="M0 0 L222.572 0 L222.572 317.165 L0 317.165 L0 0 Z" gXpos="-162.57154855643" gYpos="695.276535433069" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="C'est quoi un lo­gi­ciel libre ?"/>
                <para PARENT="Titre principal"/>
                <ITEXT CH="Un lo­gi­ciel libre c'est un pro­gramme in­for­ma­tique qui te ga­ran­ti ces 4 li­ber­tés fon­da­men­tales :"/>
                <para PARENT="Labeur"/>
                <tab FONT="VG5000 Regular"/>
                <ITEXT FONT="VG5000 Regular" Unicode="55356"/>
                <ITEXT FONT="VG5000 Regular" Unicode="57235"/>
                <ITEXT CPARENT="Logiciel " CH=" Li­ber­té 0"/>
                <nbspace CPARENT="Logiciel "/>
                <ITEXT CPARENT="Logiciel " CH=":"/>
                <ITEXT CH=" la li­ber­té de faire fonc­tion­ner le pro­gramme comme tu le sou­haite, pour n'im­porte quel usage ; "/>
                <para PARENT="Labeur"/>
                <tab/>
                <ITEXT FONT="VG5000 Regular" Unicode="55356"/>
                <ITEXT FONT="VG5000 Regular" Unicode="57235"/>
                <ITEXT CH=" "/>
                <ITEXT CPARENT="Logiciel " CH="Li­ber­té"/>
                <nbspace CPARENT="Logiciel "/>
                <ITEXT CPARENT="Logiciel " CH="1"/>
                <nbspace/>
                <ITEXT CH=": la li­ber­té d'étu­dier le fonc­tion­ne­ment du pro­gramme et de le mo­di­fier pour qu'il fasse les opé­ra­tions in­for­ma­tiques que tu sou­haite ; une condi­tion préa­lable est d'ac­cé­der au code sour­ce ; "/>
                <para PARENT="Labeur"/>
                <tab/>
                <ITEXT FONT="VG5000 Regular" Unicode="55356"/>
                <ITEXT FONT="VG5000 Regular" Unicode="57235"/>
                <ITEXT CH=" "/>
                <ITEXT CPARENT="Logiciel " CH="Li­ber­té"/>
                <nbspace CPARENT="Logiciel "/>
                <ITEXT CPARENT="Logiciel " CH="2"/>
                <nbspace/>
                <ITEXT CH=": la li­ber­té d'en re­dis­tri­buer des co­pies pour ai­der les autres ; "/>
                <para PARENT="Labeur"/>
                <tab/>
                <ITEXT FONT="VG5000 Regular" Unicode="55356"/>
                <ITEXT FONT="VG5000 Regular" Unicode="57235"/>
                <ITEXT CH=" "/>
                <ITEXT CPARENT="Logiciel " CH="Li­ber­té"/>
                <nbspace CPARENT="Logiciel "/>
                <ITEXT CPARENT="Logiciel " CH="3"/>
                <nbspace/>
                <ITEXT CH=": la li­ber­té de dis­tri­buer des co­pies de tes ver­sions mo­di­fiées aux autres ; ce fai­sant, tu donnes à l'en­semble de la com­mu­nau­té une chance de bé­né­fi­cier de tes mo­di­fi­ca­tions ;"/>
                <nbspace/>
                <ITEXT CH="une condi­tion préa­lable est d'ac­cé­der au code source."/>
                <trail PARENT="Labeur"/>
            </StoryText>
        </PAGEOBJECT>
