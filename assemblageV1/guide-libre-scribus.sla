        <PAGEOBJECT XPOS="315.433700787402" YPOS="4506.92818897638" OwnPage="13" ItemID="207812240" PTYPE="4" WIDTH="164.094488188976" HEIGHT="515.275590551181" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L164.094 0 L164.094 515.276 L0 515.276 L0 0 Z" copath="M0 0 L164.094 0 L164.094 515.276 L0 515.276 L0 0 Z" gXpos="315.433700787402" gYpos="4506.92818897638" gWidth="0" gHeight="0" PSTYLE="Labeur" LAYER="0" NEXTITEM="-1" BACKITEM="207840560"/>
        <PAGEOBJECT XPOS="676.483569553806" YPOS="4506.92818897638" OwnPage="14" ItemID="207840560" PTYPE="4" WIDTH="222.57217847769" HEIGHT="515.275590551181" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L222.572 0 L222.572 515.276 L0 515.276 L0 0 Z" copath="M0 0 L222.572 0 L222.572 515.276 L0 515.276 L0 0 Z" gXpos="676.483569553806" gYpos="4506.92818897638" gWidth="0" gHeight="0" PSTYLE="Labeur" LAYER="0" NEXTITEM="207812240" BACKITEM="-1">
            <StoryText>
                <DefaultStyle PARENT="Labeur"/>
                <ITEXT FONT="VG5000 Regular" Unicode="55357"/>
                <ITEXT FONT="VG5000 Regular" Unicode="56536"/>
                <ITEXT CH=" Pages"/>
                <para PARENT="Titre principal"/>
                <ITEXT CH="Ga­ba­rits"/>
                <para PARENT="Titre partie"/>
                <ITEXT CH="Pour le mo­ment, tu ne dois avoir que des pages blanches. Ar­ran­geons ça comme il faut avec les ga­ba­rits."/>
                <para PARENT="Labeur"/>
                <para PARENT="Labeur"/>
                <ITEXT CPARENT="Logiciel " FONT="VG5000 Regular" CH="⬉"/>
                <ITEXT CPARENT="Logiciel " CH=" Édi­tion &gt; Ga­ba­rits"/>
                <para PARENT="Labeur logiciel"/>
                <ITEXT CH="Dans la fe­nêtre gé­rer les ga­ba­rits, à l'aide des dif­fé­rents bou­tons tu peux en créer, en im­por­ter, en co­pier ou en sup­pri­mer. Par dé­faut, si tu as co­ché l'op­tion Pages en Vis-à-vis, tu as deux ga­ba­rits : Nor­mal droite et Nor­mal gauche. Ils sont ap­pli­qués res­pec­ti­ve­ment sur toutes tes pages de droite et de gauche."/>
                <para PARENT="Labeur"/>
                <para PARENT="Labeur"/>
                <ITEXT CH="Faire une grille"/>
                <para PARENT="Titre partie"/>
                <ITEXT CH="Le mieux pour créer une mise en page digne de ce nom c'est de faire une grille :"/>
                <para PARENT="Labeur"/>
                <ITEXT CPARENT="Logiciel " FONT="VG5000 Regular" CH="⬉"/>
                <ITEXT CH=" Page &gt; Ges­tion des re­pères"/>
                <para PARENT="Labeur logiciel"/>
                <ITEXT CPARENT="Logiciel " FONT="VG5000 Regular" CH="⬉"/>
                <ITEXT CH=" "/>
                <ITEXT CPARENT="Logiciel " CH="On­glet simple"/>
                <ITEXT CH=" : per­met de pla­cer pré­ci­sém­ment un re­père unique."/>
                <para PARENT="Labeur"/>
                <ITEXT CPARENT="Logiciel " FONT="VG5000 Regular" CH="⬉"/>
                <ITEXT CH=" "/>
                <ITEXT CPARENT="Logiciel " CH="On­glet Co­lonne/Ligne"/>
                <ITEXT CH=" : per­met de créer fa­ci­le­ment une dis­po­si­tion de re­pères"/>
                <para PARENT="Labeur"/>
                <tab/>
                <ITEXT FONT="VG5000 Regular" CH="✓"/>
                <ITEXT CPARENT="Logiciel " CH="Nombre de re­pères"/>
                <para PARENT="Labeur"/>
                <tab/>
                <ITEXT FONT="VG5000 Regular" CH="✓"/>
                <ITEXT CPARENT="Logiciel " CH="Gout­tière"/>
                <ITEXT CH=" : écart entre chaque co­lonne ou ligne créer"/>
                <para PARENT="Labeur"/>
                <tab/>
                <ITEXT FONT="VG5000 Regular" CH="✓"/>
                <ITEXT CPARENT="Logiciel " CH="Ori­gine"/>
                <ITEXT CH=" : dé­cou­page de la page à par­tir du for­mat to­tal ou des marges in­ternes. Pri­vi­lé­gie l'op­tion Marges pour avoir un dé­cou­page égal de la page à l'in­té­rieur de tes marges."/>
                <para PARENT="Labeur"/>
                <tab/>
                <ITEXT FONT="VG5000 Regular" CH="✓"/>
                <ITEXT CH=" "/>
                <ITEXT CPARENT="Logiciel " CH="Ap­pli­quer à toutes les pages "/>
                <ITEXT CH=": à ne faire uni­que­ment que si tu sou­haites la même grille par­tout. "/>
                <para PARENT="Labeur"/>
                <para/>
                <breakframe/>
                <ITEXT CH="Nu­mé­ro­ter les pages"/>
                <para PARENT="Titre partie"/>
                <ITEXT CH="Le nu­mé­ro de page est le plus sou­vent un in­va­riant qui se re­trouve sur toutes les pages. Il faut donc le pla­cer sur un ga­ba­rit."/>
                <para PARENT="Labeur"/>
                <ITEXT CH="Une fois dans l'es­pace d'édi­tion des ga­ba­rits,  crée un bloc texte là où tu sou­haites dis­po­ser le nu­mé­ro."/>
                <para PARENT="Labeur"/>
                <para PARENT="Labeur"/>
                <ITEXT CH="En­suite, in­sère un ca­rac­tère spé­cial qui af­fi­che­ra au­to­ma­ti­que­ment le nu­mé­ro de la page."/>
                <para PARENT="Labeur"/>
                <para PARENT="Labeur"/>
                <ITEXT CH="In­sé­rer &gt; Ca­rac­tère &gt; Nu­mé­ro de page"/>
                <para PARENT="Labeur logiciel"/>
                <para/>
                <ITEXT CH="Ges­tion des pages"/>
                <para PARENT="Titre partie"/>
                <para PARENT="Labeur logiciel"/>
                <ITEXT CH="La ges­tion des pages s'ef­fec­tue à l'aide du menu dé­rou­lant "/>
                <ITEXT CPARENT="Logiciel " CH="Page"/>
                <ITEXT CH=" et à l'aide de la fe­nêtre "/>
                <ITEXT CPARENT="Logiciel " CH="Or­ga­ni­ser les pages"/>
                <ITEXT CH=". Pour af­fi­cher cette fe­nêtre :"/>
                <para PARENT="Labeur"/>
                <para PARENT="Labeur logiciel"/>
                <ITEXT CH="Fe­nêtre &gt; Or­ga­ni­sez les pages"/>
                <para PARENT="Labeur logiciel"/>
                <para/>
                <ITEXT CH="Dans cette fe­nêtre vous pou­vez ajou­ter de nou­velles pages à par­tir de ga­ba­rits ou ap­pli­quer de ga­ba­rits en glis­sant-dé­po­sant les élé­ments. Vous pou­vez aus­si ré­oga­ni­ser les pages avec la même mé­ca­nique."/>
                <para/>
                <ITEXT CH="Pour une ma­ni­pu­la­tion ou l'ajout en quan­ti­té de plu­sieurs pages, je te conseille de pas­ser par le menu Page. Tout y est ex­pli­cite, je te laisse re­gar­der."/>
                <trail/>
