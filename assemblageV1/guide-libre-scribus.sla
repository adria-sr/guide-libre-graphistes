                <para PARENT="Labeur logiciel"/>
                <ITEXT CPARENT="Logiciel " CH="⬉ Objet &gt; Liens de cadre de texte &gt; Délier les cadre de texte (U)"/>
                <para PARENT="Labeur logiciel"/>
                <para PARENT="Labeur"/>
                <ITEXT CH="Pour visualiser les liens entre les bloc texte"/>
                <para PARENT="Labeur"/>
                <ITEXT CPARENT="Logiciel " FONT="VG5000 Regular" CH="⬉"/>
                <ITEXT CPARENT="Logiciel " CH=" Affichage &gt; Cadre de texte &gt; Afficher les liens entre les cadres"/>
                <para PARENT="Labeur logiciel"/>
                <para PARENT="Labeur"/>
                <ITEXT CH="Grille d'interlignage"/>
                <para PARENT="Labeur"/>
                <ITEXT CPARENT="Logiciel " FONT="VG5000 Regular" CH="⬉"/>
                <ITEXT CPARENT="Logiciel " CH=" Affigache &gt; Cadre de texte &gt; Grille d'interlignage"/>
                <para PARENT="Labeur logiciel"/>
                <ITEXT CH="La valeur de la grille d'interlignage doit correspondre au maximum à l'interlignage du texte de labeur. En fonction des autres niveau de lecture, tu peux la diviser par deux."/>
                <para PARENT="Labeur"/>
                <ITEXT CH="La question de la grille d'interlignage est importante pour avoir du texte toujours aligné horizontalement sur le même ligne virtuelle. Cela demande du temps au début pour un ajustement optimal."/>
                <trail/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="676.345936790146" YPOS="2601.10236220473" OwnPage="8" ItemID="201618592" PTYPE="4" WIDTH="222.57217847769" HEIGHT="515.275590551177" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L222.572 0 L222.572 515.276 L0 515.276 L0 0 Z" copath="M0 0 L222.572 0 L222.572 515.276 L0 515.276 L0 0 Z" gXpos="676.345936790146" gYpos="3236.37763779527" gWidth="0" gHeight="0" PSTYLE="Labeur" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
