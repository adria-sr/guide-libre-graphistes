                <para PARENT="Labeur"/>
                <ITEXT FONT="VG5000 Regular" CH="✓"/>
                <tab/>
                <ITEXT CPARENT="Logiciel " CH="Taille :"/>
                <ITEXT CH=" for­mat fi­nal sou­hai­té de l'édi­tion, au choix par­mi des stan­dards ou per­son­na­li­sable."/>
                <para PARENT="Labeur"/>
                <ITEXT CH="Orien­ta­tion : Pay­sage/Por­trait"/>
                <para PARENT="Labeur"/>
                <ITEXT FONT="VG5000 Regular" CH="✓"/>
                <ITEXT CH=" "/>
                <ITEXT CPARENT="Logiciel " CH="Uni­té par dé­faut "/>
                <ITEXT CH=": à choi­sir en fonc­tion de tes pré­fé­rences. "/>
                <para PARENT="Labeur"/>
                <ITEXT FONT="VG5000 Regular" CH="✓"/>
                <tab/>
                <ITEXT CPARENT="Logiciel " CH="Marges :"/>
                <ITEXT CH=" marges af­fi­chées sur ton do­cu­ment"/>
