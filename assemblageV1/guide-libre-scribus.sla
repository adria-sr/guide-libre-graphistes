        <PAGEOBJECT XPOS="676.483569553806" YPOS="686.276535433071" OwnPage="2" ItemID="201487360" PTYPE="4" WIDTH="222.57217847769" HEIGHT="394.546321709786" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L222.572 0 L222.572 394.546 L0 394.546 L0 0 Z" copath="M0 0 L222.572 0 L222.572 394.546 L0 394.546 L0 0 Z" gXpos="676.483569553806" gYpos="686.276535433071" gWidth="0" gHeight="0" PSTYLE="Labeur" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle PARENT="Labeur"/>
                <ITEXT FONT="VG5000 Regular" Unicode="55357"/>
                <ITEXT FONT="VG5000 Regular" Unicode="56393"/>
                <ITEXT CH=" À pro­pos de ce guide"/>
                <para PARENT="Titre principal"/>
                <tab/>
                <ITEXT CH="Un pro­blème avec ton crack du crea­tive cloud ? Une en­vie de re­prendre la main sur tes ou­tils de créa­tion ? Un pro­jet col­la­bo­ra­tif de jour­nal pour ton asso ? Le libre est fait pour toi !"/>
                <para PARENT="Labeur"/>
                <tab/>
                <ITEXT CH="Ins­pi­ré de mon ex­pé­rience, le dé­rou­lé pra­tique de ce guide est une in­vi­ta­tion à le re­pro­duire pour ap­prendre les bases de Scri­bus, un lo­gi­ciel libre de PAO (Pu­bli­ca­tion As­sis­tée par Or­di­na­teur)."/>
                <para PARENT="Labeur"/>
                <tab/>
                <ITEXT CH="Son am­bi­tion n'est pas d'ex­pli­quer ex­haus­ti­ve­ment toutes les fonc­tio­na­li­tés de Scri­bus, mais de trans­mettre une mé­tho­do­lo­gie d'usage du lo­gi­ciel. Pour re­pro­duire tout les élé­ments que tu vois dans ce guide, il te fau­dra sans doute cher­cher un peu par toi même. Si tu as l'ex­pé­rience d'autres lo­gi­ciels si­mi­laire ou même de trai­te­ment de texte plus cou­rants, tout de­vrait bien se pas­ser."/>
                <para PARENT="Labeur"/>
                <tab/>
                <ITEXT CH="Néanmoins, libre à toi de modifier, compléter ou détourner le contenu et la forme de ce guide."/>
                <para PARENT="Labeur"/>
                <tab/>
                <trail PARENT="Labeur"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="676.483569553806" YPOS="5142.20346456693" OwnPage="16" ItemID="201490192" PTYPE="4" WIDTH="222.57217847769" HEIGHT="515.275590551183" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L222.572 0 L222.572 515.276 L0 515.276 L0 0 Z" copath="M0 0 L222.572 0 L222.572 515.276 L0 515.276 L0 0 Z" gXpos="676.483569553806" gYpos="1965.82551181102" gWidth="0" gHeight="0" PSTYLE="Labeur" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
